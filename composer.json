{
    "name": "inweb/proclinic",
    "type": "project",
    "description": "ProClinic site",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "path",
            "url": "./admin-components/ResourceTools/Metadata"
        },
        {
            "type": "path",
            "url": "./admin-components/Fields/FeaturesField"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-curl": "*",
        "ext-json": "*",
        "fideloper/proxy": "^4.0",
        "inweb/admin": "dev-master",
        "inweb/base": "dev-master",
        "inweb/media": "dev-master",
        "inweb/admin-permissions-tool": "dev-master",
        "inweb/admin-resource-tool-images": "dev-master",
        "inweb/admin-resource-tool-metadata": "dev-master",
        "inweb/admin-features-field": "*",
        "inweb/admin-translatable-phrases": "dev-master",
        "laravel-notification-channels/telegram": "^0.0.6",
        "laravel/framework": "5.8.*",
        "laravel/horizon": "^3.2",
        "laravel/telescope": "^2.0",
        "laravel/tinker": "^1.0",
        "spatie/eloquent-sortable": "^3.7"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
